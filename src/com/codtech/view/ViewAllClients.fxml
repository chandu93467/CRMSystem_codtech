<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml" fx:controller="com.codtech.view.ViewAllClientsController">
    <TableView fx:id="clientsTable">
        <columns>
            <TableColumn text="ID" fx:id="idColumn" prefWidth="50"/>
            <TableColumn text="First Name" fx:id="firstNameColumn" prefWidth="100"/>
            <TableColumn text="Last Name" fx:id="lastNameColumn" prefWidth="100"/>
            <TableColumn text="Email" fx:id="emailColumn" prefWidth="200"/>
            <TableColumn text="Phone" fx:id="phoneColumn" prefWidth="100"/>
            <TableColumn text="Address" fx:id="addressColumn" prefWidth="200"/>
        </columns>
    </TableView>
    <Button text="Back" onAction="#handleBack"/>
</VBox>
